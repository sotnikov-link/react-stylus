{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-console": 0,
    "no-alert": 0,
    "new-cap": [ 2, { "capIsNewExceptions": ["Immutable.Map", "Immutable.Set", "Immutable.List"] } ]
  },
  "parser": "babel-eslint"
}